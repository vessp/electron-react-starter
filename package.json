{
  "name": "starter-project",
  "productName": "Starter Project",
  "version": "1.0.0",
  "description": "Starter Project Description",
  "main": "main.js",
  "scripts": {
    "test": "npm test",
    "show": "electron ./app/main.js",
    "watch:js": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
    "watch:css": "node-sass -wr app/scss/app.scss -o public/css/",
    "start": "concurrently --kill-others \"npm run watch:js\" \"npm run watch:css\" \"npm run show\"",
	"gen:dir": "mkdir public public\\js public\\css",
    "run:css": "node-sass app/scss/app.scss -o public/css/"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^4.0.0-alpha.4",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.3",
    "electron": "^1.4.4",
    "electron-packager": "^8.1.0",
    "electron-reload": "^1.0.2",
    "fs": "0.0.2",
    "jquery": "^2.2.3",
    "lodash": "^4.16.4",
    "node-sass": "^3.10.1",
    "react": "^0.14.8",
    "react-autocomplete": "^1.0.0-rc2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-sound": "^0.4.0",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "soundmanager2": "^2.97.20150601-a",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "concurrently": "^3.0.0",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-bower": "0.0.13",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.3.2",
    "postcss-cli": "^2.6.0",
    "redux-devtools": "^3.3.1"
  }
}
